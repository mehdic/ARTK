{
  "name": "@artk/core-autogen",
  "version": "0.1.0",
  "description": "ARTK AutoGen Core - Deterministic test generation engine for Playwright E2E tests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js",
      "require": "./dist-cjs/cli/index.js"
    },
    "./variants": {
      "types": "./dist/variants/index.d.ts",
      "import": "./dist/variants/index.js",
      "require": "./dist-cjs/variants/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "artk-autogen": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "dist-cjs",
    "dist-legacy-16",
    "dist-legacy-14",
    "README.md",
    "package-legacy-16.json",
    "package-legacy-14.json"
  ],
  "keywords": [
    "artk",
    "playwright",
    "test-generation",
    "e2e",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.10",
    "fast-glob": "^3.3.3",
    "ts-morph": "^24.0.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@artk/core": ">=1.0.0",
    "@playwright/test": ">=1.33.0"
  },
  "peerDependenciesMeta": {
    "@artk/core": {
      "optional": true
    },
    "@playwright/test": {
      "optional": true
    }
  },
  "artkVariant": {
    "id": "modern-esm",
    "nodeRange": [
      "18",
      "19",
      "20",
      "21",
      "22"
    ],
    "playwrightVersion": "1.57.x",
    "moduleSystem": "esm",
    "tsTarget": "ES2022"
  },
  "private": true
}