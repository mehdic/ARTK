    },
    "./utils": {
      "require": "./dist-legacy-16/utils/index.cjs",
      "types": "./dist-legacy-16/utils/index.d.ts"
    },
    "./detection": {
      "require": "./dist-legacy-16/detection/index.cjs",
      "types": "./dist-legacy-16/detection/index.d.ts"
