  "artkVariant": {
    "id": "legacy-16",
    "nodeRange": ["16", "18", "20"],
    "playwrightVersion": "1.49.x",
    "moduleSystem": "cjs",
    "tsTarget": "ES2021"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "automation",
    "artk",
    "regression-testing",
    "legacy",
    "node16"
  ],
  "author": "ARTK Team",
  "license": "MIT"
