  "files": [
    "dist-legacy-16",
    "templates",
    "version.json",
    "README.md"
  ],
  "dependencies": {
    "@playwright/test": "~1.49.0",
    "otplib": "^12.0.1",
    "strip-json-comments": "^5.0.3",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
