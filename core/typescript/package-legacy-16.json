{
  "name": "@artk/core",
  "version": "1.0.0",
  "description": "ARTK Core v1 - Vendorable Playwright infrastructure library (Legacy Node 16 variant)",
  "type": "commonjs",
  "main": "./dist-legacy-16/index.cjs",
  "types": "./dist-legacy-16/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist-legacy-16/index.cjs",
      "types": "./dist-legacy-16/index.d.ts"
    },
    "./config": {
      "require": "./dist-legacy-16/config/index.cjs",
      "types": "./dist-legacy-16/config/index.d.ts"
    },
    "./auth": {
      "require": "./dist-legacy-16/auth/index.cjs",
      "types": "./dist-legacy-16/auth/index.d.ts"
    },
    "./fixtures": {
      "require": "./dist-legacy-16/fixtures/index.cjs",
      "types": "./dist-legacy-16/fixtures/index.d.ts"
    },
    "./locators": {
      "require": "./dist-legacy-16/locators/index.cjs",
      "types": "./dist-legacy-16/locators/index.d.ts"
