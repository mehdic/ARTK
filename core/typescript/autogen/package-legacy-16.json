{
  "name": "@artk/core-autogen",
  "version": "0.1.0",
  "description": "ARTK AutoGen Core - Deterministic test generation engine (Legacy Node 16 variant)",
  "main": "dist-legacy-16/index.js",
  "types": "dist-legacy-16/index.d.ts",
  "type": "commonjs",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.legacy-16.json && npm run copy-templates:legacy-16",
    "copy-templates:legacy-16": "mkdir -p dist-legacy-16/codegen/templates && cp src/codegen/templates/*.ejs dist-legacy-16/codegen/templates/"
  },
  "bin": {
    "artk-autogen": "./dist-legacy-16/cli/index.js"
  },
  "files": [
    "dist-legacy-16",
    "README.md"
  ],
  "keywords": [
    "artk",
    "playwright",
    "test-generation",
    "e2e",
    "automation",
    "legacy",
    "node16"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.10",
    "fast-glob": "^3.3.3",
    "ts-morph": "^24.0.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@artk/core": ">=1.0.0",
    "@playwright/test": "~1.49.0"
  },
  "peerDependenciesMeta": {
    "@artk/core": {
      "optional": true
    },
    "@playwright/test": {
      "optional": true
    }
  },
  "artkVariant": {
    "id": "legacy-16",
    "nodeRange": ["16", "17", "18", "19", "20"],
    "playwrightVersion": "1.49.x",
    "moduleSystem": "cjs",
    "tsTarget": "ES2021"
  }
}
