{
  "name": "@artk/core",
  "version": "1.0.0",
  "description": "ARTK Core v1 - Vendorable Playwright infrastructure library (Legacy Node 14 variant)",
  "type": "commonjs",
  "main": "./dist-legacy-14/index.cjs",
  "types": "./dist-legacy-14/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist-legacy-14/index.cjs",
      "types": "./dist-legacy-14/index.d.ts"
    },
    "./config": {
      "require": "./dist-legacy-14/config/index.cjs",
      "types": "./dist-legacy-14/config/index.d.ts"
    },
    "./auth": {
      "require": "./dist-legacy-14/auth/index.cjs",
      "types": "./dist-legacy-14/auth/index.d.ts"
    },
    "./fixtures": {
      "require": "./dist-legacy-14/fixtures/index.cjs",
      "types": "./dist-legacy-14/fixtures/index.d.ts"
    },
    "./locators": {
      "require": "./dist-legacy-14/locators/index.cjs",
      "types": "./dist-legacy-14/locators/index.d.ts"
