      "require": "./dist-legacy-14/schemas/index.cjs",
      "types": "./dist-legacy-14/schemas/index.d.ts"
    },
    "./compat": {
      "require": "./dist-legacy-14/compat/index.cjs",
      "types": "./dist-legacy-14/compat/index.d.ts"
    },
    "./validation": {
      "require": "./dist-legacy-14/validation/index.cjs",
      "types": "./dist-legacy-14/validation/index.d.ts"
    },
    "./grid": {
      "require": "./dist-legacy-14/grid/index.cjs",
      "types": "./dist-legacy-14/grid/index.d.ts"
    },
    "./llkb": {
      "require": "./dist-legacy-14/llkb/index.cjs",
      "types": "./dist-legacy-14/llkb/index.d.ts"
    },
    "./templates/*": "./templates/*"
