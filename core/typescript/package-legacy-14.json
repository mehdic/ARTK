    "./assertions": {
      "require": "./dist-legacy-14/assertions/index.cjs",
      "types": "./dist-legacy-14/assertions/index.d.ts"
    },
    "./data": {
      "require": "./dist-legacy-14/data/index.cjs",
      "types": "./dist-legacy-14/data/index.d.ts"
    },
