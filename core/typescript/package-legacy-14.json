    "./errors": {
      "require": "./dist-legacy-14/errors/index.cjs",
      "types": "./dist-legacy-14/errors/index.d.ts"
    },
    "./utils": {
      "require": "./dist-legacy-14/utils/index.cjs",
      "types": "./dist-legacy-14/utils/index.d.ts"
    },
    "./detection": {
      "require": "./dist-legacy-14/detection/index.cjs",
      "types": "./dist-legacy-14/detection/index.d.ts"
    },
    "./targets": {
      "require": "./dist-legacy-14/targets/index.cjs",
      "types": "./dist-legacy-14/targets/index.d.ts"
