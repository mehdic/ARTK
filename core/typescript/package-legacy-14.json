  "files": [
    "dist-legacy-14",
    "templates",
    "version.json",
    "README.md"
  ],
  "dependencies": {
    "@playwright/test": "~1.33.0",
    "otplib": "^12.0.1",
    "strip-json-comments": "^5.0.3",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "artkVariant": {
    "id": "legacy-14",
    "nodeRange": ["14", "16", "18"],
    "playwrightVersion": "1.33.x",
    "moduleSystem": "cjs",
    "tsTarget": "ES2020"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "automation",
    "artk",
    "regression-testing",
    "legacy",
    "node14"
  ],
  "author": "ARTK Team",
  "license": "MIT"
}
